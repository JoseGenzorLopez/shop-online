{
  "name": "shop-online",
  "version": "1.0.0",
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "concat": "^1.0.3",
    "concurrently": "^3.6.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "node-sass": "^4.14.1",
    "postcss-cli": "^6.0.0",
    "prettier": "1.14.2",
    "react-scripts": "^1.1.5"
  },
  "dependencies": {
    "json-server": "^0.16.3",
    "prop-types": "^15.6.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^4.2.2",
    "serve": "^6.5.8",
    "styled-components": "^3.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "compile:sass": "node-sass src/sass/main.scss src/style.css",
    "watch:sass": "node-sass src/sass/main.scss src/style.css -w -r",
    "json-server": "node ./node_modules/json-server/lib/cli/bin.js --watch src/db.json -p 3001 -H 0.0.0.0",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "shop-online": "concurrently \"npm run start\" \"npm run json-server\""
  }
}
